<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pay">
        <insert id="approval" parameterType="pay">
		INSERT INTO order (id,member_id,price,payment,time) VALUES (#{partner_order_id},#{partner_user_id},#{amount.total},#{payment_method_type},now() )
	</insert>
	 <insert id="detail" parameterType="pay">
		INSERT INTO order (order_id,menu_id,name,amount,price) VALUES (#{partner_order_id},#{payment_method_type},#{item_code},#{item_name},#{quantity},#{amount.total})
	</insert>
</mapper>